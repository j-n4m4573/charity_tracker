


<div class="container">
    <div class="row">
        {{#each charity}}

      <div class="col-md-4">
        <div class="card mb-4 box-shadow">
          <img class="card-img-top" src="https://images.unsplash.com/photo-1521493959102-bdd6677fdd81?ixlib=rb-0.3.5&ixid=eyJhcHBfaWQiOjEyMDd9&s=4a027094c15a1ae14bf72f8f71d9380b&auto=format&fit=crop&w=1650&q=80" alt="Card image cap">
          <div class="card-body">
            <h4 class="card-text" name= {{charity}}>{{this.charityName}}</h4>
            <p class="card-text">{{this.irsClassification.nteeClassification}}</p>
            <div class="d-flex justify-content-between align-items-center">
              <div class="btn-group">
                <button type="button" class="btn btn-sm btn-info">Select</button>
                <button type="button" class="btn btn-sm btn-outline-info">More Info</button>
              </div>
              <small class="text-muted">9 mins</small>
            </div>
          </div>
        </div>
      </div>
      {{/each}}
